.loadingBox{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  background-color: #fff;
  justify-content: center;
  flex-direction: column;
  position: absolute;
  top: 0;
  left: 0;
}
.loadingBox .loadingBcenter{
  width: 60rpx;
  height: 60rpx;
  border-radius: 100%;
  background-color: #DEEAEB;
  position: relative;
  animation: loading 1s linear infinite;
  display: flex;
  box-shadow: 0 0 20rpx rgba(119,130,145,.5);
}
.loadingBox .loadingBcenter .loading-move{
  width: 30rpx;
  height: 60rpx;
  border-top-left-radius: 60rpx;
  border-bottom-left-radius: 60rpx;
  background-color: #08AFAA;
  display: inline-block;
}
.loadingBox .loadingBcenter .loadingcover{
  width: 30rpx;
  height: 60rpx;
  background-color: inherit;
  border-top-right-radius: 60rpx;
  border-bottom-right-radius: 60rpx;
  display: inline-block;
  transform-origin: left;
}
.loadingBox .loadingBcenter .center-cover{
  width: 80%;
  height: 80%;
  background-color: #fff;
  position: absolute;
  top: 10%;
  left: 10%;
  border-radius: 100%;
}
.loadingBox text{
  font-size: 25rpx;
  color: #768190;
  margin-top: 30rpx;
}
@keyframes loading{
  0%{
    transform: rotateZ(0);
  }
  100%{
    transform: rotateZ(360deg);
  }
}