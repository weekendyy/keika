<script>
  import wepy from 'wepy'
  import CardModel from './Card-model'
  import wxParse from '../../wxParse/wxParse'
  import MyModel from '@/pages/My/index-model'
  import BackHome from '../../components/backHome'
  import cheaderModel from '@/components/cheader-model'
  import Address from '../../utils/address.js'
  export default class extends wepy.page {
    config = {
      navigationBarBackgroundColor: "#fff",
      navigationBarTitleText: '集卡活动详情',
      enablePullDownRefresh: true,
      backgroundTextStyle: 'dark'
    }
    components = {
      BackHome: BackHome
    }
    data = {
      lucky_goods_id: null,
      goodsInfo: null,
      cardStatus: false,
      userInfo: null,
      loadingHidden: false,
      queryData: {},
      LuckStatus: true,
      target: '',
      likeNum:'',
      actionSheetHidden: true,
      showPosterBox: false,
      pricePre:'',
      posterImg: '',
      isOpen: false
    }
    methods = {
      onShareAppMessage(){
        let that = this;
        let shareTitle = null;
        let path='/pages/Card/details?employer_user_id='+that.goodsInfo.user_id+"&"+'id='+that.queryData.lucky_goods_id
        shareTitle = that.goodsInfo.activity_name
        let shareObj = {
          title: shareTitle,
          path: path,
          success: ()=>{
            wx.showToast({
              title:'转发成功',
              icon:'success ',
              duration:1500
            })
            this.actionSheetHidden = true
            this.$apply()
          }
        }
        return shareObj
      },
      gotomy(){
        wx.navigateTo({
          url: '../Card/coupon?id='+this.goodsInfo.lucky_order_id
        })
      },
      gotoIndex(){
        wx.switchTab({
          url: '/pages/Index/index'
        })
      },
      closeShareBox(){
        this.actionSheetHidden =  true
        this.$apply()
      },
      getUserInfo(res){
        wx.showLoading({
          title: '生成卡片中',
        })
        let posterPic = wx.getStorageSync('posterPic_'+'luckyCanvas'+'_'+this.goodsInfo.id)
        if(posterPic){
          this.posterImg = posterPic
          this.showPosterBox = true
          wx.hideLoading()
          this.$apply()
          return false
        }
        let param = {}
        param.id = this.goodsInfo.id
        param.type = 4
        this.actionSheetHidden =  true
        CardModel.getPostData(param,ResData=>{
          if(ResData.code == 1){
            CardModel.creatPoster(this, 'luckyCanvas', ResData.data, this.goodsInfo.activity_name, '0.00', this.pricePre,'集卡','../../images/posterTy.png','',this.goodsInfo.id)
          }
        })
      },
      closePosterBox(){
        this.showPosterBox = false
        this.$apply()
      },
      savePostePic(){
        CardModel.savePoste(this,'luckyCanvas',this.goodsInfo.id)
      },
      open(){
        this.isOpen = true
        this.cardStatus = true
        let parm = {
          lucky_goods_id: this.queryData.lucky_goods_id
        }
        CardModel.getLuckDraw(parm, resData=>{
          this.LuckStatus = false
          this.isOpen = false
          if(Number(resData.code)===1){
            let k = resData.data.key
            this.goodsInfo.check_collect_status = resData.data.check_collect_status
            this.target = resData.data.rund_str
            for(let i = 0;i < this.goodsInfo.character_rule.length;i++) {
              if(k === this.goodsInfo.character_rule[i].key){
                this.goodsInfo.character_rule[i].status = 1
                ++this.goodsInfo.character_rule[i].get_num
                --this.goodsInfo.raffle_num
              }
            }
            this.cardStatus = false
            this.$apply()
          } else {
            CardModel.showTips(resData.message)
          }
        })
      },
      add(res){
        Address.getUser(res,()=>{
          this.goodsInfo.first_join = 2
          this.$apply()
        })
      },
      closeLuck(){
        this.LuckStatus = true
      },
      exchange(){
        CardModel.exchange_goods({lucky_goods_id: this.queryData.lucky_goods_id},(ResData)=>{
          if(ResData.code == 1 && ResData.data == 1){
            wx.showToast({
              title: '已发送个人中心',
              icon: 'success',
              duration: 2000,
            })
            this.goodsInfo.check_exchange_statu = 1
            this.goodsInfo.lucky_order_status = 1
            this.goodsInfo.lucky_order_id = ResData.lucky_order_id
          } else {
            wx.showToast({
              title: ResData.data.message,
              icon: 'none',
              duration: 1500
            })
          }
          this.$apply()
        })
      },
      makePhoneCall(){
        MyModel.makePhoneCall(this.goodsInfo.link_tel)
      },
      toMap(){
        MyModel.openMap(this.goodsInfo.longitude,this.goodsInfo.lucky_shop_name,this.goodsInfo.address)
      }
    }
    onLoad(options){
      this.pricePre = options.pricePre
      if(options.employer_user_id){
        this.queryData.employer_user_id = options.employer_user_id
      }
      this.queryData.lucky_goods_id = options.id
      if(options.formID){
        this.queryData.magic_form_id = options.formID
      }
    }
    onShow(){
      CardModel.getDetail(this.queryData, ResData=> {
        if(ResData.code == 1){
          this.goodsInfo = ResData.data
          const article = ResData.data.prize_detail
          wxParse.wxParse('article', 'html', article, this, 0)
          this.likeNum = ResData.data.browse_num
          this.loadingHidden = true
        }
        this.$apply()
      })
    }
    onPullDownRefresh(){
      CardModel.getDetail(this.queryData, ResData=> {
        if(ResData.code == 1){
          this.goodsInfo = ResData.data
          wx.stopPullDownRefresh()
        }
        this.$apply()
      })
    }
  }
</script>
