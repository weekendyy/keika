<template>
  <web-view src="{{URL}}"></web-view>
</template>

<script>
  import wepy from 'wepy'
  export default class index extends wepy.page {
    data = {
      URL: '',
    }
    config = {
      navigationBarBackgroundColor: '#000000',
      navigationBarTextStyle: '#fff'
    }
    onLoad(options){
      this.URL = options.url;
      this.$apply()
    }
  }
</script>
